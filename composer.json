{
    "name": "linuxside/linuxside.com-website",
    "type": "project",
    "description": "The linuxside.com website source code",
    "keywords": [
        "blog"
    ],
    "require": {
        "php": "^7.3",
        "league/commonmark": "^1.6",
        "symfony/cache": "^5.2",
        "symfony/console": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/error-handler": "^5.2",
        "symfony/finder": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/var-dumper": "^5.2",
        "twig/twig": "^3.3",
        "voku/portable-ascii": "^1.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
